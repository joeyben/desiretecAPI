{
    "name": "viral/laravel-adminpanel",
    "description": "The Laravel AdminPanel with API Boilerplate.",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate",
        "adminpanel",
        "rest-api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "arcanedev/log-viewer": "^4.5",
        "arcanedev/no-captcha": "^5.0",
        "barryvdh/laravel-ide-helper": "^2.5",
        "creativeorange/gravatar": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "dimsav/laravel-translatable": "^10.0",
        "doctrine/dbal": "^2.6",
        "dompdf/dompdf": "^0.8.2",
        "laravel/categories": "*",
        "fideloper/proxy": "~4.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "hieu-le/active": "^3.5",
        "intervention/image": "^2.4",
        "jdavidbakr/mail-tracker": "~2.1",
        "laracasts/generators": "^1.1",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lord/laroute": "2.*",
        "maatwebsite/excel": "^3.1",
        "mariuzzo/laravel-js-localization": "^1.4",
        "mcamara/laravel-localization": "^1.3",
        "mobiledetect/mobiledetectlib": "^2.8",
        "mpdf/mpdf": "^7.1.9",
        "nicolaslopezj/searchable": "1.*",
        "nwidart/laravel-modules": "^3.2",
        "orangehill/iseed": "^2.6",
        "overtrue/laravel-lang": "~3.0",
        "phpro/soap-client": "^1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.3",
        "rinvex/cacheable": "v0.0.5",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-newsletter": "4.2.3",
        "spatie/laravel-translatable": "^2.2",
        "spatie/laravel-translation-loader": "2.2",
        "squizlabs/php_codesniffer": "^3.3",
        "tecnickcom/tcpdf": "^6.2",
        "torann/geoip": "^1.0",
        "tymon/jwt-auth": "dev-develop",
        "unisharp/laravel-filemanager": "~1.8",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "codedungeon/phpunit-result-printer": "^0.6.0",
        "bvipul/generator": "dev-master",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^4.0.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0",
        "xethron/migrations-generator": "2.0.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ],
        "files": [
            "tests/Utilities/helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./laravel-categories",
            "options": {
                "symlink": false
            }
        }
    ]
}
